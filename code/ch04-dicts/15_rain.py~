#!/usr/bin/env python3


rainfall = {} < 1 >

while True:
    city_name = input("Enter city name: ")
    if not city_name:
        break

    mm_rain = input("Enter mm rain: ") < 2 >
    rainfall[city_name] = rainfall.get(city_name, 0) + int(mm_rain) < 3 >

for city, rain in rainfall.items():
    print(f"{city}: {rain}")
